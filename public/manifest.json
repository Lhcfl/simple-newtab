{
  "name": "Simple Newtab 1.1",
  "description": "Maybe the simplest new tab",
  "version": "1.1",
  "manifest_version": 3,
  "permissions": [
    "storage",
    "activeTab",
    "scripting",
    "bookmarks",
    "topSites",
    "favicon"
  ],
  "host_permissions": [
    "https://*/*",
    "http://*/*"
  ],
  "optional_host_permissions": [
    "file:///"
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "16": "./icon.png",
    "32": "./icon.png",
    "48": "./icon.png",
    "128": "./icon.png"
  }
}